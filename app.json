{
  "expo": {
    "name": "commumoto",
    "slug": "my-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logomoto.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logomoto.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logomoto.png",
        "backgroundColor": "#ffffff"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBFDUJ7l1k8uusWP-zOVgfBkD3jZwfz8jc"
        }
      },
      "package": "com.chgni.motoesimproj"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "befef251-9055-419f-8109-abbd34c3b919"
      }
    },
    "plugins": [
      [
      "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ]
    ]
  }
}
